<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="filter_select_accounts" model="ir.filters">
            <field name="name">Account filter</field>
            <field name="model_id">account.move.line</field>
            <field name="domain">
                    ['|','|','|','|','|',
                    ('account_id', '=like', '3%'),
                    ('account_id', '=like', '4%'),
                    ('account_id', '=like', '5%'),
                    ('account_id', '=like', '6%'),
                    ('account_id', '=like', '7%'),
                    ('account_id', '=like', '8%')
                    ]
            </field>
            <field name="user_id" eval="False" />
        </record>
    </data>
</odoo>
